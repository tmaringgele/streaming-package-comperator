{
  "version": 2,
  "builds": [
    {
      "src": "FrontEnd/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "BackEnd/api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "BackEnd/api.py"
    },
    {
      "src": "/(.*)",
      "dest": "FrontEnd/build/$1"
    }
  ]
}
